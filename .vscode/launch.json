{
  "configurations": [
    {
      "name": "Python 调试程序: 模块",
      "type": "debugpy",
      "request": "launch",
      "module": "enter-your-module-name"
    },
    {
      "name": "inference_d2",
      "type": "debugpy",
      "request": "launch",
      "module": "videopose3d.infer_video_d2",
      "cwd": "/workspace/code",
      "console": "integratedTerminal",
    },
    {
      "name": "prepare_data",
      "type": "debugpy",
      "request": "launch",
      "module": "prepare_dataset.main_chunk",
      "cwd": "/workspace/code",
      "console": "integratedTerminal",
      "args": [
        "task=detectron2"
      ]
    },
    {
      "name": "run_custom",
      "type": "debugpy",
      "request": "launch",
      "module": "videopose3d.run",
      "cwd": "/workspace/code",
      "console": "integratedTerminal",
    },
    {
      "name": "triangulate",
      "type": "debugpy",
      "request": "launch",
      "module": "triangulation.main",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "calibration",
      "type": "debugpy",
      "request": "launch",
      "module": "camera_calibration.main",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      //   "args": [
      //     "/workspace/code/camera_calibration/input_video"
      //   ]
    },
    {
      "name": "camera_position_mapping",
      "type": "debugpy",
      "request": "launch",
      "module": "triangulation.camera_position_mapping",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "sam3d_body",
      "type": "debugpy",
      "request": "launch",
      "module": "SAM3Dbody.main",
      "cwd": "/workspace/code",
      "python": "/opt/conda/envs/sam_3d_body/bin/python",
      "console": "integratedTerminal",
      "args": [
        "infer.gpu=0",
      ]
    },
  ]
}